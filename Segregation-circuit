#include <Adafruit_LiquidCrystal.h>
Adafruit_LiquidCrystal lcd_1(0);
float moisture=0;
void setup()
{
  pinMode(A0,INPUT);
  pinMode(2,OUTPUT);
  lcd_1.begin(16,2);
  Serial.begin(9600);
}

void loop()
{
 digitalWrite(2,HIGH);
 delay(10);
 moisture=analogRead(A0);
 digitalWrite(2,LOW);
 Serial.println(moisture);
 if(moisture<=359.00)
 {
   lcd_1.print("Dry waste");
   delay(1000);
 }
  else
  {
    lcd_1.print("Wet waste");
    delay(1000);
  }
  lcd_1.clear();
}
